{
  "functions": {
    "app/brands/[slug]/page.tsx": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/brands/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
